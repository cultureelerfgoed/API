              "type": "string",
              "example": "10134675"
            }
          },
          {
            "name": "heeftNaam",
            "in": "query",
            "description": "Name of the world heritage site",
            "required": false,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "string",
              "example": "Hollandse Waterlinies"
            }
          },
          {
            "name": "registratiedatum",
            "in": "query",
            "description": "Registration date of the world heritage site",
            "required": false,
            "style": "form",
