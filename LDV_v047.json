{
  "openapi": "3.0.3",
  "info": {
    "title": "Linked Data API RCE",
    "description": "Linked data on cultural-historical objects. This concerns built and archaeological national monuments, findings, discovery locations, complexes, archaeological complexes, research areas, and sites.\nAccess to the Linked Data API of the RCE does not require registration. Use the API in a responsible and lawful manner. Respect the applicable laws and regulations regarding the use of cultural heritage data. These terms may be changed at any time. It is your responsibility to stay informed of any changes. By using the API, you agree to these terms. For questions or concerns, please contact the Cultural Heritage Agency of the Netherlands.This API (Application Programming Interface) of the RCE offers the possibility to request data from the RCE. The following data is offered via this API: \nRijksmonumenten, werelderfgoederen, gezichten\nThe API also allows filtering on the results. Filters are applied by: municipality, province, place name, and various numbering.",
    "termsOfService": "https://linkeddata.cultureelerfgoed.nl/rce/-/stories/Restfull-API",
    "contact": {
      "url": "https://linkeddata.cultureelerfgoed.nl",
      "email": "thesauri@cultureelerfgoed.nl"
    },
    "license": {
      "name": "Open Data Commons Attribution License 1.0",
      "url": "https://opendatacommons.org/licenses/by/1-0/"
    },
    "version": "0.3.1"
  },
  "servers": [
    {
      "url": "https://api.linkeddata.cultureelerfgoed.nl/queries/rce"
    }
  ],
  "tags": [
    {
      "name": "Rijksmonumenten",
      "description": "Information concerning one or more instances of a built or archaeological national monument \"rijksmonument\". National heritage sites are historical buildings, archaeological sites or man-made green structures like parks."
    },
    {
      "name": "Werelderfgoed",
      "description": "Information regarding one or more instances of a World Heritage Site \"werelderfgoed\". World Heritage Sites are monuments so important to the global community that we want to pass them on safely to future generations."
    },
    {
      "name": "Gezicht",
      "description": "Information on one or more instances of a \"gezicht\". A protected town or village scape is an area in a town or village with a special cultural-historical character. Protection aims to preserve the cultural-historical identity of an area and use it in developments."
    }
  ],
  "externalDocs": {
    "description": "json-ld frames",
    "url": "https://www.w3.org/TR/json-ld11-framing/"
  },
